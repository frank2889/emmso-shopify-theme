{% layout none %}

{% assign current_path = request.path | downcase %}
{% if current_path contains '/da/pages/sitemap-pages-da' %}

  {% assign base_url = shop.url %}
  {% assign locale = "da" %}

  {% for page in pages %}
    {% unless page.handle contains 'sitemap' %}
